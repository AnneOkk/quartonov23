	journaltitle = {Robotics \& {AI} Ethics},
	author = {Lee, Aeri},
	urldate = {2023-03-16},
	date = {2021-09},
	file = {Snapshot:/Users/annekleine/Zotero/storage/XIY3BBDP/4010028517150.html:text/html},
