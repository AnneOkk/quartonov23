	shortjournal = {Insights Imaging},
	author = {Codari, Marina and Melazzini, Luca and Morozov, Sergey P. and van Kuijk, Cornelis C. and Sconfienza, Luca M. and Sardanelli, Francesco and {European Society of Radiology (ESR)}},
	urldate = {2022-08-02},
	date = {2019-10-31},
	langid = {english},
	keywords = {Radiology, Artificial Intelligence, Machine Learning, Radiologists, Surveys and Questionnaires},
