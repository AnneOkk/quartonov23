	number = {4},
	journaltitle = {European Radiology},
	shortjournal = {Eur Radiol},
	author = {Pinto dos Santos, D. and Giese, D. and Brodehl, S. and Chon, S. H. and Staab, W. and Kleinert, R. and Maintz, D. and Baeßler, B.},
	urldate = {2022-08-01},
	date = {2019-04},
