	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/10705511.2019.1588124},
	keywords = {feature selection, high-dimensional data, Mediation analysis},
	file = {Full Text PDF:/Users/annekleine/Zotero/storage/LPE674SU/van Kesteren and Oberski - 2019 - Exploratory Mediation Analysis with Many Potential.pdf:application/pdf},
}

