	keywords = {Machine learning, Big data, {AI}, Mental health, Depression, Psychiatry, Precision medicine, Decision trees, {fMRI}, {ADHD}, Algorithms, Alzheimer, Data mining, {DSM}-5. Schizophrenia, {IBM} Watson, Mental disease, {MRI}, Neuro networking, Random forests, {RDoC}, Research domain criteria, Support vector machines},
